html
  include partials/head.pug

  body
    h1= showdata.title

    h2 Developed By
    ul
      for author in showdata.developedBy
        li= author

    h2 Starring
    ul
      for star in showdata.starring
        li= star

    h2 Seasons
    ul
      for seasonNum in showdata.seasons
        li
          a(href=`/seasons/${seasonNum.number}`)= `Seasons ${seasonNum.number}`